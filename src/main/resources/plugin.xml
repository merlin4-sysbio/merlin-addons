<plugin start="true">
	<name>merlin-addons</name>
	<uid>pt.uminho.ceb.biosystems.merlin.addons</uid>
	<version>4.0.05</version>

	<dependencies>
		<dependency uid="merlin" />
	</dependencies>

	<extensions>

		<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% - compartments - %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->

		<extension uid="aibench.core"
			name="aibench.core.operation-definition"
			class="pt.uminho.ceb.biosystems.merlin.merlin_addons.compartments.MergeCompartments">
			<operation-description
				name="merge compartments" path="30@model/30@tools"
				uid="operations.MergeCompartments.ID" />
		</extension>

		<extension uid="aibench.workbench"
			name="aibench.workbench.view">
			<icon-operation
				operation="operations.MergeCompartments.ID"
				icon="icons/Lifecycle/remote.png" />
		</extension>

		<extension uid="aibench.workbench"
			name="aibench.workbench.view">

			<gui-operation
				operation="operations.MergeCompartments.ID"
				class="pt.uminho.ceb.biosystems.merlin.merlin_addons.compartments.MergeCompartmentsGUI" />
		</extension>

		<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% - clone workspace - %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->


		<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% - id converter - %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
		<extension uid="aibench.core"
			name="aibench.core.operation-definition"
			class="pt.uminho.ceb.biosystems.merlin.merlin_addons.ids_converter.CompoundAndReactionsConverter">
			<operation-description name="converter"
				path="40@validation/" uid="operations.CompoundAndReactionsConverter.ID" />
		</extension>

		<extension uid="aibench.workbench"
			name="aibench.workbench.view">
			<icon-operation
				operation="operations.CompoundConverter.ID"
				icon="icons/Lifecycle/remote.png" />
		</extension>

	</extensions>
</plugin>